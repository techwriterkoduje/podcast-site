{
  "name": "tech-writer-koduje",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn full-build && docusaurus start",
    "build": "yarn full-build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "get-episodes": "npx tsx src/scripts/save-all-episode-data.ts -y",
    "create-search-index": "npx tsx src/scripts/create-search-index.ts -y",
    "full-build": "yarn get-episodes && docusaurus build && yarn create-search-index && docusaurus build"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.1",
    "@docusaurus/preset-classic": "3.9.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/quicksand": "^5.2.10",
    "@mdx-js/react": "^3.0.0",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "clsx": "^2.0.0",
    "fuse.js": "^7.1.0",
    "jsdom": "^27.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-timer-hook": "^4.0.5",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.1",
    "@docusaurus/tsconfig": "3.9.1",
    "@docusaurus/types": "3.9.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.6.2",
    "@types/xmldom": "^0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  },
  "packageManager": "yarn@4.10.3"
}
